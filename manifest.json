{
    "manifest_version":2,
    "name": "Zendesk Refresher",
    "version": "1.0",
    "description" : "An extension for refreshing Zendesk Queue",
    "background": {
        "html":["background.html"],
        "scripts": ["background.js"]
    },
    "permissions":["activeTab"],
    "browser_action": {
        "default_popup":"popup.html"
    },
    "icons": {
        "32": "32.png",
        "128": "128.png"
     }
}